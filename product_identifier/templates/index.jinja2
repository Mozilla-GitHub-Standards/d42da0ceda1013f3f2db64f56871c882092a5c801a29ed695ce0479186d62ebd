<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="{{ url_for('static', filename='bootstrap-3.3.6-dist/css/bootstrap.min.css')}}">
    <link rel="stylesheet" href="{{ url_for('static', filename='bootstrap-3.3.6-dist/css/bootstrap-theme.min.css')}}">
    <title>Product Identifier Status</title>
  </head>
  <body>
    <div class="container">
      <h1>Product Identifier URLs</h1>

      <h3>Status</h3>
      <div class="col-md-4">
        <table class="table">
          <thead>
            <tr>
              <th>Product URLs</th>
              <th>Total URLs</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>{{ product_count }}</td>
              <td>{{ total_count }}</td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
  </body>
</html>
